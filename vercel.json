{
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "build": {
    "env": {
      "VITE_API_URL": "https://mileapp-api.up.railway.app",
      "VITE_APP_ENV": "production"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}